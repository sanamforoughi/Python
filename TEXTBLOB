from textblob import TextBlob 

# next work to synonymise = http://rwet.decontextualize.com/book/textblob/

#.classifier is a classifier haha


wiki = TextBlob("#brosavethechildren #Brofist #ONEWORLD #helpingbuds #Respect @pewdiepie The reason y ur so great is you give so much back to ur fans !! <3") 
print(wiki.tags) 
print(wiki.sentiment) 
print(wiki.word_counts)
print(wiki.words)
